  "scripts": {
    "setup": "pnpm install",
    "clean": "pnpm rimraf ./quasar",
    "dev": "pnpm clean && pnpm quasar dev",
    "dev:sass": "APP_VARIANT=default pnpm dev",
    "dev:manager": "APP_VARIANT=manager PORT=4581 pnpm dev",
    "dev:android": "pnpm dev -m capacitor -T android",
    "dev:pwa": "pnpm dev -m pwa",
    "dev:ssr": "pnpm dev -m ssr",
    "build": "pnpm clean && pnpm quasar build",
    "build:manager": "APP_VARIANT=manager pnpm build",
    "build:android": "pnpm build -m capacitor -T android",
    "build:pwa": "pnpm build -m pwa",
    "build:ssr": "pnpm build -m ssr",
    "generate:icon": "icongenie g -i ./src/assets/icon.png --theme-color #3f51b5 --splashscreen-color #3f51b5",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0"
  },
